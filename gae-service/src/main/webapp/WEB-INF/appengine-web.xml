<?xml version="1.0" encoding="UTF-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
    <application>layaraca</application>
    <version>1</version>
    <system-properties>
        <property name="in.gae.j" value="true" />
    </system-properties>
    <sessions-enabled>true</sessions-enabled>
    <static-files>
        <include path="/**.css" />
        <include path="/**.js" />
        <include path="/**.png" />
        <include path="/**.gif" />
        <exclude path="/**.html" />
    </static-files>
</appengine-web-app>
